{
	"name": "nextjs-sprout",
	"version": "0.1.0",
	"description": "The ByteRunners NextJS Sprout",
	"author": {
		"name": "ByteRunners",
		"email": "runner@ByteRunners.com",
		"url": "https://github.com/ByteRunners"
	},
	"license": "MIT",
	"homepage": "https://ByteRunners.run",
	"repository": {
		"type": "git",
		"url": "https://github.com/ByteRunners/Next.js-Sprout",
		"directory": "Next.js-Sprout",
		"branch": "main",
		"commit": "HEAD",
		"host": "github.com"
	},
	"bugs": {},
	"private": true,
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=6.0.0",
		"npm": "please-use-pnpm-instead",
		"yarn": "please-use-pnpm-instead"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"commit": "cz",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0 --ignore-path .gitignore",
		"lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\"",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"prepare": "husky install"
	},
	"dependencies": {
		"next": "14.0.3",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.2",
		"@commitlint/config-conventional": "^18.4.2",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.54.0",
		"eslint-config-next": "14.0.3",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-jest-dom": "^5.1.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-tailwindcss": "^3.13.0",
		"eslint-plugin-testing-library": "^6.1.2",
		"husky": "^8.0.3",
		"postcss": "^8",
		"prettier": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"tailwindcss": "^3.3.5",
		"typescript": "^5"
	},
	"keywords": [
		"react",
		"nextjs",
		"typescript",
		"tailwindcss",
		"next",
		"template",
		"sprout",
		"byterunners",
		"pnpm"
	],
	"packageManager": "pnpm@8.10.5",
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"prettier . --write --check",
			"eslint . --cache --fix --ext .jsx --ext .js --ext .ts --ext .tsx"
		]
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
